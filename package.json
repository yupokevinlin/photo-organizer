{
  "name": "photo-organizer",
  "productName": "Photo Organizer",
  "version": "0.0.1",
  "description": "Photo Organizer",
  "main": "./build/main.js",
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage",
    "build": "npm run build-electron && npm run build-renderer",
    "build-electron": "cross-env NODE_ENV=production webpack --config webpack.electron.prod.config.js",
    "build-renderer": "cross-env NODE_ENV=production webpack --config webpack.renderer.prod.config.js",
    "start-dev": "cross-env START_HOT=1 npm run start-renderer",
    "start-renderer": "webpack-dev-server --config webpack.renderer.dev.config.js",
    "start-electron": "webpack --config webpack.electron.dev.config.js && electron ./build/main.js",
    "prestart": "npm run build",
    "start": "electron .",
    "build-installer": "electron-builder"
  },
  "build": {
    "files": [
      "./assets/*",
      "./build/*",
      "dist/**/*",
      "package.json"
    ],
    "appId": "photo-organizer",
    "win": {
      "target": [
        "portable"
      ],
      "icon": "./build/assets/icon.ico",
      "requestedExecutionLevel": "requireAdministrator"
    },
    "nsis": {
      "installerIcon": "./build/assets/icon.ico",
      "uninstallerIcon": "./build/assets/icon.ico",
      "uninstallDisplayName": "Photo Organizer",
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "author": "Kevin Lin",
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "excel-column-name": "^1.0.1",
    "fs-readdir-recursive": "^1.1.0",
    "immer": "^9.0.1",
    "moment": "^2.29.1",
    "react": "^16.12.0",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^16.12.0",
    "react-redux": "^7.2.3",
    "redux": "^4.0.5",
    "redux-immer": "^1.0.5",
    "redux-saga": "^1.1.3",
    "styled-components": "^5.0.1",
    "xlsx": "^0.16.9"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@hot-loader/react-dom": "^16.11.0",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/jest": "^24.9.1",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-test-renderer": "^16.9.2",
    "@types/styled-components": "^4.4.3",
    "@types/webdriverio": "^4.13.3",
    "@types/webpack-env": "^1.15.1",
    "@typescript-eslint/parser": "^2.20.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^5.2.1",
    "css-loader": "^3.4.2",
    "electron": "^8.0.1",
    "electron-builder": "^22.10.5",
    "electron-devtools-installer": "^2.2.4",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.4.0",
    "file-loader": "^5.0.2",
    "fork-ts-checker-webpack-plugin": "^1.6.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.3",
    "image-webpack-loader": "^6.0.0",
    "jest": "^24.9.0",
    "jest-styled-components": "^6.3.4",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "react-hot-loader": "^4.12.19",
    "redux-devtools-extension": "^2.13.8",
    "sass-loader": "^8.0.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.1.3",
    "ts-jest": "^24.3.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.5",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-env": "^0.8.0",
    "webpack-merge": "^4.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
